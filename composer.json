{
    "name": "testmeister/igorvicente",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.0",
        "zendframework/zend-component-installer": "^1.0",
        "zendframework/zend-mvc": "^3.0.1",
        "zendframework/zend-db": "^2.11",
        "zendframework/zend-form": "^2.11",
        "zendframework/zend-debug": "^2.6",
        "zendframework/zend-mvc-i18n": "^1.1",
        "zendframework/zend-authentication": "^2.7",
        "zendframework/zend-expressive-session": "^1.3",
        "zendframework/zend-session": "^2.9",
        "zendframework/zend-mvc-plugin-identity": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/",
            "Auth\\": "module/Auth/src/",
            "Task\\": "module/Task/src/",
            "User\\": "module/User/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationTest\\": "module/Application/test/",
            "Auth\\": "module/Auth/test/",
            "Task\\": "module/Task/test/",
            "User\\": "module/User/test/"
        }
    },
    "extra": [],
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "zendframework/zend-test": "^3.2",
        "squizlabs/php_codesniffer": "^3.3",
        "symfony/var-dumper": "^5.4"
    }
}
